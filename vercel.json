{
    "framework": "nextjs",
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate, no-store"
                },
                {
                    "key": "Clear-Site-Data",
                    "value": "\"cache\""
                }
            ]
        }
    ]
}